{
  "name": "List (small thumbnails)",
  "package": "com.fliplet.list-small-thumbs",
  "version": "1.1.0",
  "icon": "img/icon.png",
  "tags": ["type:component", "category:list"],
  "provider_only": false,
  "html_tag": "div",
  "providers": [
    "com.fliplet.link",
    "com.fliplet.image-manager"
  ],
  "interface": {
    "dependencies": [
      "fliplet-core",
      "fliplet-studio-ui",
      "jquery-ui",
      "lodash",
      "handlebars",
      "bootstrap"
    ],
    "assets": [
      "js/interface.js",
      "css/interface.css"
    ]
  },
  "build": {
    "dependencies": [
      "fliplet-core",
      "bootstrap-css",
      "font-awesome",
      "lodash",
      "list-swipe",
      "mixitup"
    ],
    "assets": [
      "js/build.js",
      "css/build.css"
    ]
  },
  "references": [
    "items.$.linkAction.page:page",
    "items.$.linkAction.files.id:mediaFile",
    "items.$.imageConf.id:mediaFile"
  ]
}
